programa {
  // 5 - Fazer um algoritmo que calcule a área de um terreno quadrado.
  funcao inicio() {
    real lado, area

    escreva("\n---[ CALCULAR ÁREA DE UM TERRENO QUADRADO ]---")
    escreva("\nDigite o número dos dois lados: ")
    leia(lado)

    area = lado * lado
    
    escreva("A área dos lados deu ", area)
  }
}
