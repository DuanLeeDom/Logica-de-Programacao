programa
{

    funcao inicio()
    {
        inteiro a, b, c

        escreva("Informe o valor de A: ")
        leia(a)

        escreva("Informe o valor de B: ")
        leia(b)

        escreva("Informe o valor de C: ")
        leia(c)

        se (a+b < c)
        {
          escreva("A soma de A+B é menor que C.")
        }
        senao 
        {
          escreva("A soma de A+B não é menor que C.")
        }
    }
}
