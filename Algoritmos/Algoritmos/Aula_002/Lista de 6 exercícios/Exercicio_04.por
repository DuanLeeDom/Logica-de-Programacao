programa {
  // 4 - Fazer um algoritmo que leia um número e apresente ele ao cubo.
  funcao inicio() {
   	real n1, result
			
		escreva("\nVamos saber como é ao cubo")
    escreva("\nDigite um número: ")
    leia(n1)

    result = n1 * n1 * n1

    escreva("\nO número ", n1, " ao cubo é: ", result)
  }
}
